<%- include header.ejs %>
<h1> Index </h1>


<table class="table table-hover">
    <thead>
        <tr>
            <th>Date</th>
            <th>Trainer</th>
            <th>Group Name</th>
            <th>Room</th>
            <th>Capacity</th>
            <th>Subject</th>
        </tr>
    </thead>
    <tbody>
    <% data.results.forEach(row => { %>
        <tr>
            <td><%= row.date.toLocaleDateString()%></td>
            <td><%= row.tName%></td>
            <td><%= row.gName%></td>
            <td><%= row.rName%></td>
            <td><%= row.rCap%></td>
            <td><%= row.skill%></td>
        </tr>
    <% })%>
    </tbody>
</table>



</body>
</html>